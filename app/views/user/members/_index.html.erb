<% members.each do |member| %>
  <tr>
    <% if member.guest? %>
      <td><%= link_to member.nickname,member_path(member.id) %></td>
      <td><%= "#{member.full_name}さん" %></td>
      <td><%= "#{member.likes.count}件" %></td>
      <td><%= "#{member.followings.count}人" %></td>
      <td><%= "#{member.followers.count}人" %></td>
    <% end %>
  </tr>
<% end %>