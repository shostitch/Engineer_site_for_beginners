<% @posts.each do |post| %>
  <tr>
    <td><%= link_to post.member.nickname, member_path(post.member.id) %></td>

    <td><%= link_to post.title, post_path(post.id) %></td>

    <td><%= l post.created_at %></td>

    <td><%= post.tags.pluck(:name).join(',') %></td>

    <td id="like_buttons_<%= post.id %>">
      <%= render "user/likes/like", post: post %>
    </td>
  </tr>
<% end %>